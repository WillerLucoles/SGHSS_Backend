{
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.0",
    "prisma": "^6.9.0",
    "sequelize": "^6.37.7",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "name": "sghss_backend",
  "version": "1.0.0",
  "description": "O SGHSS é um sistema completo de gestão hospitalar desenvolvido em Node.js, projetado para facilitar o gerenciamento de pacientes, profissionais de saúde, consultas e prontuários médicos.",
  "main": "index.js",
  "type": "module",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:studio": "npx prisma studio",
    "prisma": "npm run prisma:generate && npm run prisma:migrate",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate dev",
    "test:seed": "dotenv -e .env.test -- npx prisma db seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillerLucoles/SGHSS_Backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WillerLucoles/SGHSS_Backend/issues"
  },
  "homepage": "https://github.com/WillerLucoles/SGHSS_Backend#readme",
  "devDependencies": {
    "dotenv-cli": "^10.0.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "jest": {
    "transform": {},
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.js"
    ],
     "testMatch": [
      "**/__tests__/**/*.test.js"
    ] 
  }
}
